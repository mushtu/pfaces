<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">

	<namespace>http://omidbiz.com/ui</namespace>
	<tag>
		<tag-name>datePicker</tag-name>
		<component>
			<component-type>com.omidbiz.core.component.JalaliCalendar</component-type>
			<renderer-type>com.omidbiz.core.component.JalaliCalendarRenderer</renderer-type>
		</component>
		
		<attribute>
			<description><![CDATA[Style attribute of the generated Input Tag.]]></description>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description><![CDATA[Space seperated list of style classes for the generated Input Tag.]]></description>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[]]></description>
			<name>onselect</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[]]></description>
			<name>value</name>
			<required>false</required>
			<type>java.lang.Object</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[]]></description>
			<name>disabled</name>
			<required>false</required>
			<type>boolean</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[]]></description>
			<name>showTime</name>
			<required>false</required>
			<type>boolean</type>
		</attribute>
		
	</tag>
	
	<tag>
		<tag-name>timeTable</tag-name>
		<component>
			<component-type>com.omidbiz.core.component.TimeTable</component-type>
			<renderer-type>com.omidbiz.core.component.TimeTableRenderer</renderer-type>
		</component>
		
		<attribute>
			<description><![CDATA[Style attribute of the generated Input Tag.]]></description>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description><![CDATA[Space seperated list of style classes for the generated Input Tag.]]></description>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	
		
		<attribute>
			<description><![CDATA[]]></description>
			<name>disabled</name>
			<required>false</required>
			<type>boolean</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[An org.omidbiz.core.component.CalendarModel instance representing the backed model.]]></description>
			<name>value</name>
			<required>false</required>
			<type>org.omidbiz.core.component.CalendarModel</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[An actionlistener to process when command is executed.]]></description>
			<name>actionListener</name>
			<required>false</required>
			<type>javax.faces.event.ActionListener</type>
		</attribute>
		<attribute>
			<description><![CDATA[A method expression or a string outcome to process when command is executed.]]></description>
			<name>action</name>
			<required>false</required>
			<type>javax.el.MethodExpression</type>
		</attribute>
		<attribute>
			<description><![CDATA[A client-side javascript function name (only name without parantheses) with epoch argument to get current day in
				java script you need to new Date and set this epoch argument 
			 ]]></description>
			<name>onselect</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		
		
	</tag>
	
</facelet-taglib>
